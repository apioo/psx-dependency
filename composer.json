{
  "name": "psx/dependency",
  "description": "Simple and fast DI container",
  "keywords": ["di", "dependency", "container"],
  "homepage": "http://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com/"
    }
  ],
  "require": {
    "php": ">=7.0",
    "doctrine/annotations": "^1.3",
    "psr/cache": "^1.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "psx/cache": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Dependency\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\Dependency\\Tests\\": "tests/"
    }
  },
  "provide": {
    "psr/container-implementation": "1.0"
  }
}
