{
  "name": "psx/dependency",
  "description": "Simple and fast DI container",
  "keywords": ["di", "dependency", "container"],
  "homepage": "https://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "https://chrisk.app"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/cache": "^1.0|^2.0|^3.0",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "vimeo/psalm": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "PSX\\Dependency\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\Dependency\\Tests\\": "tests/"
    }
  },
  "provide": {
    "psr/container-implementation": "2.0"
  }
}
